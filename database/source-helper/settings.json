{
	"[blade]": {
		"editor.autoClosingBrackets": "always",
		"editor.formatOnSave": false
	},
	"files.associations": {
		"*.blade.php": "blade",
		"*.php": "php"
	},
	"eslint.options": {
		"rules": {
			"no-template-curly-in-string": "off"
		}
	},
	"settingsSync.ignoredExtensions": [],
	"settingsSync.ignoredSettings": [],
	"editor.formatOnType": false,
	"diffEditor.codeLens": true,
	"tabnine.experimentalAutoImports": true,
	"beautify.language": {
		"js": {
			"type": ["javascript", "json", "jsonc"],
			"filename": [".jshintrc", ".jsbeautifyrc"]
		},
		"css": ["css", "less", "scss"],
		"html": ["htm", "html", "php", "blade.php"]
	},
	"[php]": {
		"prettier.embeddedLanguageFormatting": "auto",
		"editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.configPath": "e:/PROJECT/sistem-informasi-idep/project/.prettierrc",
	"prettier.prettierPath": "./project/node_modules/prettier",
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.bracketSameLine": true,
	"prettier.htmlWhitespaceSensitivity": "strict",
	"prettier.printWidth": 240,
	"prettier.useTabs": true,
	"prettier.withNodeModules": true,
	//
	"prettier.documentSelectors": [
		"**/*.php",
		"**/*.html",
		"**/*.js",
		"**/*.json",
		"**/*.css",
		"**/*.scss",
		"**/*.less",
		"**/*.md",
		"**/*.vue",
		"**/*.ts",
		"**/*.tsx",
		"!**/node_modules/**",
		"!**/vendor/**",
		"!**/storage/**",
		"!**/*.blade.php"
	],
	"prettier.ignorePath": ".prettierignore",
	"[scss]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[less]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"parser": "php",
	"plugins": ["@prettier/plugin-php"],
	"bladeFormatter.format.noMultipleEmptyLines": true,
	"bladeFormatter.format.noPhpSyntaxCheck": true,
	"bladeFormatter.format.wrapLineLength": 260,
	"editor.cursorStyle": "block",
	"editor.definitionLinkOpensInPeek": true,
	"editor.cursorBlinking": "smooth",
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.cursorSurroundingLinesStyle": "default",
	"prettier.endOfLine": "auto",
	"prettier.useEditorConfig": false,
	"prettier.requirePragma": false,
	// "intelephense.environment.documentRoot": "../project",
	"intelephense.diagnostics.undefinedTypes": false,
	"intelephense.diagnostics.undefinedFunctions": false,
	"intelephense.diagnostics.undefinedConstants": false,
	"intelephense.diagnostics.undefinedClassConstants": false,
	"intelephense.diagnostics.undefinedMethods": false,
	"intelephense.diagnostics.undefinedProperties": false,
	"intelephense.diagnostics.undefinedVariables": true,
	"intelephense.environment.includePaths": ["./vendor/barryvdh/laravel-ide-helper", "./vendor/laravel/framework/src/Illuminate"],
	"intelephense.diagnostics.enable": true,
	"intelephense.completion.classMemberSnippets": true,
	"intelephense.trace.server": "verbose",
	"intelephense.files.associations": ["**/*.php"],
	"editor.minimap.autohide": true,
	"editor.minimap.scale": 1,
	"editor.minimap.renderCharacters": true,
	"editor.formatOnPaste": false,
	"editor.formatOnSaveMode": "file",
	"editor.formatOnSave": true,

	"emmet.includeLanguages": {
		"blade": "html"
	},
	"blade.format.enable": true,
	"blade.format.wrapLineLength": 260,
	"blade.customDirectives": [
		"@append",
		"@auth",
		"@aware",
		"@break",
		"@can",
		"@canany",
		"@cannot",
		"@case",
		"@checked",
		"@choice",
		"@class",
		"@component",
		"@componentfirst",
		"@continue",
		"@csrf",
		"@dd",
		"@default",
		"@disabled",
		"@dump",
		"@each",
		"@else",
		"@elseauth",
		"@elsecan",
		"@elsecanany",
		"@elsecannot",
		"@elseguest",
		"@elseif",
		"@empty",
		"@endauth",
		"@endcan",
		"@endcanany",
		"@endcannot",
		"@endcomponent",
		"@endcomponentClass",
		"@endcomponentfirst",
		"@endempty",
		"@endenv",
		"@enderror",
		"@endfor",
		"@endforeach",
		"@endforelse",
		"@endfragment",
		"@endguest",
		"@endif",
		"@endisset",
		"@endlang",
		"@endonce",
		"@endphp",
		"@endprepend",
		"@endproduction",
		"@endpush",
		"@endrole",
		"@endsection",
		"@endsession",
		"@endslot",
		"@endswitch",
		"@endunless",
		"@endverbatim",
		"@endwhile",
		"@env",
		"@error",
		"@extends",
		"@extendsfirst",
		"@for",
		"@foreach",
		"@forelse",
		"@fragment",
		"@guest",
		"@hasSection"
	]
}
